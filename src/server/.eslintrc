{
  "extends": "airbnb-base",
  "env": {
    "mocha": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "import/imports-first": 0
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  }
}
